{
  "private": "true",
  "name": "concodeit-async",
  "main": "index.html",
  "scripts": {
    "prestart": "npx rimraf dist/programs && npx mkdir -p dist && sed 's/src\\/index/index/g' index.html > dist/index.html && cp -r programs dist/ && test -e src/settings.json || echo {} > src/settings.json",
    "build": "npx browserify src/index.js -t babelify --outfile dist/index.js -dv",
    "watch": "npx mkdirp dist && npx watchify src/index.js -t babelify --outfile dist/index.js -dv",
    "start": "npx concurrently \"npm run watch\" \"npx live-server --watch=dist/index.js,dist/index.html --open=dist/index.html\"",
    "devel": "npx rimraf node_modules/cycle-robot-drivers-async && ln -s ../../cycle-robot-drivers-async node_modules/cycle-robot-drivers-async"
  },
  "author": "Michael Jae-Yoon Chung",
  "license": "MIT",
  "dependencies": {
    "@cycle/dom": "22.4.0",
    "cycle-robot-drivers-async": "0.0.10",
    "node-blockly": "1.0.36",
    "util.promisify": "1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0"
  }
}
