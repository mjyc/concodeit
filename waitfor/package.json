  },
  "author": "Michael Jae-Yoon Chung",
  "license": "MIT",
  "dependencies": {
    "@cycle/dom": "^22.4.0",
    "cycle-robot-drivers-async": "0.0.6",
    "node-blockly": "1.0.36",
    "util.promisify": "1.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@cycle/time": "^0.19.0",
    "parcel-bundler": "^1.6.1",
    "xstream": "^11.11.0"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
